<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android">

	<uses-permission android:name="android.permission.INTERNET" />
	<uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
	<uses-permission android:name="android.permission.FOREGROUND_SERVICE" />
	<uses-permission android:name="android.permission.FOREGROUND_SERVICE_DATA_SYNC" />
	<uses-permission android:name="android.permission.POST_NOTIFICATIONS" />

	<application
		android:allowBackup="true"
		android:icon="@mipmap/appicon"
		android:roundIcon="@mipmap/appicon_round"
		android:supportsRtl="true"
		android:usesCleartextTraffic="true"
		android:extractNativeLibs="true">
		
		
		

		<!-- Proxy service -->
		<service
			android:name="Hillariot.BarbarisProxy.MyProxyService"
			android:exported="true"
			android:permission="android.permission.BIND_VPN_SERVICE"
			android:foregroundServiceType="dataSync">
			<intent-filter>
				<action android:name="android.net.ProxyService"/>
			</intent-filter>
		</service>

		<service
    android:name="Hillariot.BarbarisProxy.MyProxyTileService"
    android:exported="true"
    android:icon="@drawable/ic_Proxy_small"
    android:label="BarbarisProxy"
    android:permission="android.permission.BIND_QUICK_SETTINGS_TILE">
			<intent-filter>
				<action android:name="android.service.quicksettings.action.QS_TILE" />
			</intent-filter>
		</service>

	</application>
</manifest>
